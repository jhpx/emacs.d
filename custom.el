(custom-set-variables
 ;; custom-set-variables was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(custom-safe-themes (quote ("4cf3221feff536e2b3385209e9b9dc4c2e0818a69a1cdb4b522756bcdf4e00a4" default)))
 '(session-use-package t nil (session))

 ;; Code Style Standard
 '(flycheck-phpcs-standard "WordPress")

 ;; Never backup in cureent directory
 '(backup-directory-alist (quote (("." . "~/.backups"))))

 ;; The color of "80 column rule" Guides line
 '(fci-rule-color "darkblue")
 '(fci-rule-column 80)

 ;; For file encoding
 '(revert-without-query (quote (".*")))
 '(filename-coding-systems 'utf-8)
 '(coding-system-for-write 'utf-8)

 ; For tab and space
 '(x-stretch-cursor t)
 '(show-trailing-whitespace t)
 )


